{
  "name": "ts-package",
  "version": "1.0.0",
  "scripts": {
    "fix:format": "prettier --write \"**/*.ts\"",
    "fix:lint": "eslint --fix --ext .ts .",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "check:format": "prettier --check \"**/*.ts\"",
    "check:lint": "eslint --ext .ts .",
    "check": "pnpm check:lint && pnpm check:format",
    "test:coverage": "jest --collectCoverage",
    "test": "jest",
    "build": "ts-node --esm scripts/build.ts",
    "start": "ts-node src/index.ts",
    "prepublishOnly": "pnpm test && pnpm build",
    "publish": "release-it"
  },
  "license": "MIT",
  "devDependencies": {
    "@release-it/keep-a-changelog": "3.1.0",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.18",
    "@swc/helpers": "0.4.13",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "auto-changelog": "2.4.0",
    "esbuild": "0.16.13",
    "esbuild-plugin-swc": "1.0.1",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "prettier": "2.7.1",
    "release-it": "15.5.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "pipem": "0.0.3"
  }
}
